<a class="btn btn-primary" href="/usuarios/registrar">Ingresar Nuevo Usuario</a>
<br />
<br />
<h2>Lista de usuarios</h2>
<br />
<table class="table table-hover">
  <thead>
    <tr>
      <th>ID</th>
      <th>Username</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
    {{#each user}}
    <tr>
      <td><a href="/admin/usuarios/editar/{{this._id}}">{{this._id}}</a></td>
      <td>{{this.username}}</td>
      <td>{{this.email}}</td>
    </tr>
    {{/each}}
   <tbody>
</table>